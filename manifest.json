{
    "manifest_version": 2,
    "name": "foodtracker",
    "version": "0.2",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search?q=*"
            ],
            "js": [
                "forbidenIndexes.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "engine.js"
    ],
    "background": {
        "page": "background.html"
    }
}