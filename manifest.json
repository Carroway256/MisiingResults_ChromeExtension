{
    "manifest_version":2,
    "name": "foodtracker",
    "version" : "0.2",
    "omnibox": { "keyword" : "chrome" },
    "icons": {
        "16": "admin-privilege-icon.png"
      },
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>",
        "storage"

      ],
      "background" : {

          "scripts":["background.js"]
    }

}